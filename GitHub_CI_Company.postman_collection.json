		{
			"name": "Cadastro_Empresa",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// Gerando massa de dados\r",
							"pm.sendRequest({\r",
							"    url: \"https://www.5devs.com.br/api/empresa\",\r",
							"    method: 'GET',\r",
							"    header: {\r",
							"        'Content-Type': 'application/json'\r",
							"    }\r",
							"    }, function (err, res) {\r",
							"    if (err) {\r",
							"        console.log(err);\r",
							"    } else {\r",
							"        const jsonData = res.json();\r",
							"        pm.environment.set(\"nomeEmpresa\", jsonData.razaoSocial);\r",
							"        pm.environment.set(\"registro\", jsonData.cnpj);\r",
							"        pm.environment.set(\"emailCompany\",jsonData.email);\r",
							"         pm.environment.set(\"telefone\",jsonData.telefone);\r",
							"    }\r",
							"});\r",
							"\r",
							"// Gerando massa de dados\r",
							"pm.sendRequest({\r",
							"    url: \"https://www.5devs.com.br/api/pessoa\",\r",
							"    method: 'GET',\r",
							"    header: {\r",
							"        'Content-Type': 'application/json'\r",
							"    }\r",
							"    }, function (err, res) {\r",
							"    if (err) {\r",
							"        console.log(err);\r",
							"    } else {\r",
							"        const jsonData = res.json();\r",
							"        pm.globals.set(\"nominho\", jsonData.nome);\r",
							"        \r",
							"    }\r",
							"});\r",
							"\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.environment.set(\"ID_Empresa\", pm.response.json().newCompany._id);\r",
							"\r",
							"pm.test(\"Status code is 201\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"\r",
							"pm.test(\"Empresa criada com sucesso\", function () {\r",
							"    pm.expect(pm.response.json().msg).to.be.eql(`Olá a companhia ${pm.environment.get('nomeEmpresa')} foi cadastrada com sucesso.`);\r",
							"});"
						],
